#include <stdio.h>
#include <math.h>

int main()
{
	long long int num = 0, idx = 1;
	while (true)
	{
		num += idx;
		idx++;
		long long int count = 0; //count = number of divisors
		for (long long int i = 1; i <= sqrt(num); i++)
		{
			if (num % i == 0)
			{
				if (num / i == i) //i = sqrt(num)
				{
					count++;
				}
				else
				{
					count += 2;
				}
			}
		}
		if (count > 500)
			break;
	}
	printf("Result: %llu", num);
	return 0;
}
