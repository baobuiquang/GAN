#include <stdio.h>
#include <math.h>

#define target 2000000

//Function: Prime number
bool isPrime(int num)
{
	if ((num < 2) || (num >2 && num%2 == 0))
	{
		return false;
	}
	for (int i = 3; i <= sqrt(num); i += 2)
	{
		if (num % i == 0)
		{
			return false;
		}
	}
	return true;
}

int main()
{
	long long int sum = 2;
	for (int i = 3; i < target; i += 2)
	{
		if (isPrime(i) == true)
		{
			sum += i;
		}
	}
	printf("Result = %llu", sum);
}
